{
    "name": "defectivecode/faker-storage",
    "type": "laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DefectiveCode\\Faker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DefectiveCode\\Faker\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Corey McCormick",
            "email": "corey@defectivecode.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-gd": "*",
        "league/flysystem": "^3.0",
        "ramsey/uuid": "^4.9",
        "fakerphp/faker": "^1.24",
        "symfony/mime": "^7.3",
        "egulias/email-validator": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3.13",
        "laravel/pint": "^1.8",
        "symfony/var-dumper": "^7.3",
        "swoole/ide-helper": "^6.0"
    },
    "suggest": {
        "ext-swoole": "Required for Swoole worker support for parallel file generation",
        "ext-pcntl": "Required for PCNTL worker support for parallel file generation"
    }
}
